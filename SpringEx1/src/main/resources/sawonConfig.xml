<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

   <!-- SawonData설정..사원명은 생성자주입,급여입사일은 stter주입 -->
   <bean name="sdata" class="spring.ex3.di.SawonData">
      <constructor-arg value="유재석"/>
      <property name="pay" value="2500000"/>
      <property name="ipsaday" value="2020-12-12"/>
   </bean>
   <!-- Sawon SawonData주입받기-->
   <bean name="saw" class="spring.ex3.di.Sawon">
      <constructor-arg ref="sdata"/>
   </bean>
   
   
   <!-- 2번째 SawonData설정..사원명은 생성자주입,급여입사일은 stter주입 -->
   <bean name="sdata2" class="spring.ex3.di.SawonData">
      <constructor-arg value="이효리"/>
      <property name="pay" value="4500000"/>
      <property name="ipsaday" value="20250-12-22"/>
   </bean>
   <!-- Sawon SawonData주입받기-->
   <bean name="saw2" class="spring.ex3.di.Sawon">
      <constructor-arg ref="sdata2"/>
   </bean>
   
</beans>
